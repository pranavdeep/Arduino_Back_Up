void loop(void) 
{
  
  // picture loop  
  u8g.firstPage();  
  do 
  {
    draw();
  } while( u8g.nextPage() );
  delay(3000);
  
  // increase the state
  Serial.println(draw_state);
  draw_state++;
  if ( draw_state >= 9 )
    draw_state = 0;
}

  
void draw() 
{
  u8g_prepare();
  switch(draw_state) 
  {
    case 0: smile_1(); break;
    case 1: sad_1(); break;
    case 2: love_1(); break;
    case 3: boss_1(); break;
    case 4: angry_1(); break;
    case 5: annoyed_1(); break;
    case 6: blank_1(); break;
    case 7: naughty_1(); break;
    case 8: shocked_1(); break;
    
  }
}



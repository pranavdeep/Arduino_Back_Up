void draw(void) 
{
  u8g_prepare();
  switch(draw_state) {
    case 0: smile_1(); break;
    case 1: sad_1(); break;
    case 2: annoyed_1(); break;
    case 3: confused_1(); break;
    case 4: blank_1(); break;
    case 5: blank_1(); break;
    case 6: grin_1(); break;
    case 7: teeth_1(); break;
    case 8: unknown_1(); break;
    case 9: unknown2_1(); break;
    //case 10: unknown2_1(); break;
  }
  }


void setup(void) 
{
#if defined(ARDUINO)
  pinMode(13, OUTPUT);           
  digitalWrite(13, HIGH);  
#endif
Serial.begin(9600);
}

void loop(void) 
{
  
  // picture loop  
  u8g.firstPage();  
  do 
  {
    draw();
  } while( u8g.nextPage() );
  delay(3000);
  
  // increase the state
  Serial.println(draw_state);
  draw_state++;
  if ( draw_state >= 11 )
    draw_state = 0;
}


